        <assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <oracle.manageddataaccess.client>
    <version number="*">
      <dataSources>
        <dataSource alias="pdb" descriptor="(DESCRIPTION=(ADDRESS=(PROTOCOL=tcp)(HOST=[hostname or IP])(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=[service name]))) "/>
      </dataSources>
    </version>
  </oracle.manageddataaccess.client>

